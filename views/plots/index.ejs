<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <%- include ('../partials/head.ejs') %>
  </head>
  <body>
    <nav>
      <%- include ('../partials/nav.ejs') %>
    </nav>
    <div class="container">
      <h1>Plots</h1>
      <a href="/plots/new">New Plot</a>

      <h2>Plots entries are displayed here</h2>
      <ul>
        <% for (let plot of plots) { %>
          <li>
            <a href="/plots/<%=plot.id%>">
              <%=plot.name%>
            </a>
          </li>
        <% } %>
      </ul>
    </div>
    <div class="buttons">
      <a href="/elements">Elements Page</a>
    </div>
  </body>
</html>

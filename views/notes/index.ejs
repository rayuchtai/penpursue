<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <%- include ('../partials/head.ejs') %>
  </head>
  <body>
    <nav>
      <%- include ('../partials/nav.ejs') %>
    </nav>
    <div class="container">
      <h1>Notes</h1>
      <a href="/notes/new">New Note</a>
      <h2>Notes are displayed here:</h2>
      <ul>
        <% for(let note of notes) { %>
          <li>
            <a href="/notes/<%=note.id%>">
              <%=note.name%>
            </a>
          </li>
          <% } %>
      </ul>
    </div>
    <div class="buttons">
      <a href="/elements">Elements Page</a>
    </div>
  </body>
</html>
